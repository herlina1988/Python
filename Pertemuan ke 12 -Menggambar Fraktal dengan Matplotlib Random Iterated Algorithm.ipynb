   "source": [
    "n = int(input('Enter the number of points in the Fern: '))\n",
    "x, y = draw_fern(n)\n",
    "# Plot the points\n",
    "plt.plot(x, y, '.')\n",
    "plt.title('Fern with {0} points'.format(n))\n",
    "plt.show()"
   ]
